{
  "packages": [
    "packages/*"
  ],
  "version": "5.22.1-alpha.3",
  "command": {
    "version": {
      "exact": true,
      "push": false
    },
    "publish": {
      "registry": "https://registry.npmjs.org",
      "message": "chore: publish release %s\n",
      "ignoreChanges": [
        "packages/*/README.md",
        "packages/*/package-lock.json",
        "packages/*/dist",
        "packages/*/**/*.spec.ts",
        "packages/*/**/*.snap"
      ]
    }
  }
}
